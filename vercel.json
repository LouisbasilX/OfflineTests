{
  "version": 2,
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.9"
    }
  },
  "rewrites": [
    { "source": "/api/test/create", "destination": "/api/test/create.rs" },
    { "source": "/api/test/fetch", "destination": "/api/test/fetch.rs" },
    { "source": "/api/test/submit", "destination": "/api/test/submit.rs" },
    { "source": "/api/admin/flush", "destination": "/api/admin/flush.rs" }
  ]
}